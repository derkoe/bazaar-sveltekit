{
	"routes": [
		{
			"route": "/build/*"
		},
		{
			"route": "/static/*"
		},
		{
			"route": "/favicon.ico"
		},
		{
			"route": "/.auth/*"
		},
		{
			"route": "/*",
			"rewrite": "/api/azure"
		}
	],
	"navigationFallback": {
		"rewrite": "/api/azure"
	},
	"platform": {
		"apiRuntime": "node:16"
	},
	"auth": {
		"identityProviders": {
			"azureActiveDirectory": {
				"registration": {
					"openIdIssuer": "https://login.microsoftonline.com/0f6f68be-4ef2-465a-986b-eb9a250d9789/v2.0",
					"clientIdSettingName": "AZURE_CLIENT_ID",
					"clientSecretSettingName": "AZURE_CLIENT_SECRET"
				}
			}
		}
	}
}
